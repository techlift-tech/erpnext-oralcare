 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n{% if letter_head and not no_letterhead %}\n<div class=\"letter-head\">{{ letter_head }}</div>\n{% endif %} {% if max_pages > 1 %}\n<p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n{% endif %} {%- endmacro -%} {{ add_header(0,1,doc,letter_head, no_letterhead) }}\n<div>\n    <div align=\"right\">\n        <div><span align=\"left\" style=\"font-size: 30px;\"> INVOICE</span></div>\n    </div>\n    <div align=\"right\">\n        <div><span align=\"left\" style=\"font-size: 16px;\">{{doc.name}}</span></div>\n  \n    </div>\n    <br>\n    <table style=\"width:100%\" class=\"\">\n        <tr style=\"width:100%\">\n            <td align=\"left\" style=\"width:50%\">\n                <div id=\"container\" align=\"left\" style=\"width:100%\">\n                    <table style=\"width:90%\">\n                          <tr>\n                            <td>Patient ID</td>\n                            <td>{{ doc.patient }}</td>\n                        </tr>\n                       \n                       \n                        <tr>\n                            <td>Address</td>\n                            <td>{%- if doc.customer_address -%} {{ doc.customer_address }} {%- else -%}\n                                {%- endif -%}{%- if doc.address_display -%} {{ doc.address_display }} {%- else -%}\n                                {%- endif -%}</td>\n                        </tr>\n\n                    </table>\n                </div>\n            </td>\n            <td align=\"right\" style=\"width:50%\">\n                <div id=\"container\" align=\"left\" style=\"width:100%\">\n                    <table align=\"right\" style=\"width:90%\">\n                        \n                            <tr>\n                            <td>Patient Name</td>\n                            <td>{{ doc.patient_name.upper() }}</td>\n                        </tr>\n                        <tr>\n                            <td>Date</b>\n                            </td>\n                            <td>{{doc.get_formatted(\"posting_date\")}}</td>\n                        </tr>\n\n                    </table>\n                </div>\n            </td>\n        </tr>\n    </table>\n    <table class=\"table table-bordered table-condensed\">\n        <thead>\n            <tr>\n                <th style=\"width: 40px\" class=\"table-sr\">Sr</th>\n\n                <th style=\"width: 70%px; \" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    Item</th>\n\n                <th style=\"width: 10%px; \" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    HSN</th>\n\n                <th style=\"width: 10%px;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    Qty</th>\n\n                <th style=\"width: 10%px; \" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    Rate</th>\n\n                <th style=\"width: 10%px;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    Discount</th>\n                <th style=\"width: 10%px; \" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    Amount</th>\n            </tr>\n        </thead>\n        <tbody>\n            {%- for row in doc.items -%}\n            <tr>\n                <td class=\"table-sr\">1</td>\n\n                <td border: 1px solid #999; class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    <div class=\"value\">\n\n                        <div>\n                            <div class=\"primary\">\n\n                                {{ row.item_code }}\n\n                            </div>\n                        </div>\n                </td>\n\n                <td class=\"text-left\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    <div class=\"value\">\n\n                        {{ row.gst_hsn_code }}\n                    </div>\n                </td>\n\n                <td class=\"text-right\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    <div class=\"value\">\n                        {{ row.qty }}\n                    </div>\n                </td>\n\n                <td class=\"text-right\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    <div class=\"value\">\n                        {{ frappe.utils.fmt_money(row.rate, currency=\"\u20b9\") }}\n                    </div>\n                </td>\n\n                <td class=\"text-right\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    <div class=\"value\">\n                        {{frappe.utils.fmt_money(row.discount_amount, currency=\"\u20b9\")}}\n                    </div>\n                </td>\n\n                </td>\n\n                <td class=\"text-right\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                    <div class=\"value\">\n                        {{frappe.utils.fmt_money(row.amount, currency=\"\u20b9\")}}\n                    </div>\n                </td>\n            </tr>\n            {%- endfor -%}\n        </tbody>\n    </table>\n\n    <table>\n        <td align=\"right\" style=\"width:10%\">\n            <div id=\"container\" align=\"right\" style=\"width:100%\">\n                <table align=\"right\" style=\"width:60%\">\n                    <tr>\n                        <td>Total</b>\n                        </td>\n                        <td align=\"right\">{{frappe.utils.fmt_money(doc.total, currency=\"\u20b9\")}}</td>\n                    </tr>\n                    <tr>\n                        <td>Additional Discount</b>\n                        </td>\n                        <td align=\"right\">{{frappe.utils.fmt_money(doc.discount_amount, currency=\"\u20b9\")}}</td>\n                    </tr>\n                    {%- for row in doc.taxes -%}\n                    {%- if (not row.included_in_print_rate or doc.flags.show_inclusive_tax_in_print) and row.tax_amount != 0 -%}\n                    <tr>\n                        <td class=\"text-left\">\n                            {{ row.description }}\n                        </td>\n                        <td class=\"text-right\">\n                            {{ row.get_formatted(\"tax_amount\", doc) }}\n                        </td>\n                    <tr>\n                        {%- endif -%} {%- endfor -%}\n                    <tr>\n                        <td>Grand Total</b>\n                        </td>\n                        <td align=\"right\">{{frappe.utils.fmt_money(doc.grand_total, currency=\"\u20b9\")}}</td>\n                    </tr>\n                    <tr>\n                        <td>RoundedTotal</b>\n                        </td>\n                        <td align=\"right\">{{frappe.utils.fmt_money(doc.rounded_total, currency=\"\u20b9\")}}</td>\n                    </tr>\n                    <tr>\n                        <td>In Words</b>\n                        </td>\n                        <td align=\"right\">{{doc.in_words}}</td>\n                    </tr>\n\n                </table>\n    </table>\n</div>\n\n{% if doc.other_charges_calculation %} {{doc.other_charges_calculation}} {% endif %}\n <div align=\"left\">\n        <div><span align=\"left\" style=\"font-size: 16px;\"><br><br> Declaration:This is system generated invoice does not require Signature</span></div>\n    </div>", 
